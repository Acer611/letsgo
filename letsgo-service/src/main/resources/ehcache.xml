<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false">
	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="userCache" maxElementsInMemory="1000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="studentCache" maxElementsInMemory="1000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="oauth2ClientCache" maxElementsInMemory="1000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="oauth2TokenCache" maxElementsInMemory="1000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="oauth2AuthCache" maxElementsInMemory="1000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="oauth2AuthRefreshCache" maxElementsInMemory="1000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="oauth2RefreshTokenCache" maxElementsInMemory="1000" eternal="false"
		   timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" />
	<cache name="TlsSignCache" maxElementsInMemory="5000" eternal="false"
		   timeToIdleSeconds="86400" timeToLiveSeconds="86400" overflowToDisk="false" />
</ehcache>