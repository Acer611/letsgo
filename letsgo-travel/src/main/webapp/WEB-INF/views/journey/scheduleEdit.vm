                        newDateEdit = getNewDay(newDateEdit, (msumlength - 1));
                    }
                    for (var i = msumlength; i < diffnumedits; i++) {
                        newDateEdit = getNewDay(newDateEdit, 1);
                        addDaynum(i, newDateEdit);
                        if(currentdayNum>0) {
                            $(".sui-steps").find(".wrap").each(function (i) {
                                if (i == (msumlength + 1)) {
                                    $(this).find("div").removeClass().addClass("current");
                                }
                                if (i > (msumlength + 1)) {
                                    $(this).find("div").removeClass("current").addClass("todo");

                                }
                                if ($(this).children("div").hasClass("current")) {
                                    var _ind = $(this).index();
                                    $("#editArea>div.box").eq(_ind).show().siblings(".box").hide();
                                }
                            })
                        }
                    }
                }
            }
        }

