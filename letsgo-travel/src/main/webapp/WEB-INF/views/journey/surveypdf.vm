#set($pageTitle="团队行程管理")
#set($layout="/WEB-INF/views/common/layout/layout.vm")
#set($scriptUrls=["$!{STATIC_URL}/resources/plugins/iCheck/icheck.min.js","$!{STATIC_URL}/resources/dist/js/diaoyan.js","$!{STATIC_URL}/resources/plugins/blockUI/jquery.blockUI.js"])
<style type="text/css">
    ul,li{
        list-style: none;
    }
    h3{font-size:18px;}
    .quesNum{
        margin:0;
    }
    .quesNum ul li .box{
        border-top:0;
    }
    .quesNum ul li .box h3{
        font-weight: 900;
    }
    .tab-pane{
        width:100%;
        min-height:500px;
    }
    .question{
        border-bottom: 1px solid #ccc;
    }
    .quesNum .question:last-child{
        border-bottom:0;
    }
    .quesNum{
        width:100%;
        min-height:100px;

    }
    .question .quesTitle{
        font-weight: 900;
        font-size: 16px;
    }
    .question .quesTitle1{
        font-weight: 900;
        font-size: 18px;
        margin-top:5px;
    }
    .col-two{
        margin-left:10px;
    }
    .col-two li{
        line-height:20px;
    }
    .additem{
        width:100px;
        height:34px;
        line-height: 34px;
        padding:0 0 0 20px !important;
        font-weight:100;
    }

    .tab-pane-two div{
        text-align: center;
        margin-top:20px;
    }
    .tab-pane-two div img{
        width:150px;
        height:200px;
    }
    .tab-pane-two div p{
        text-align: center;
        line-height:30px;
        margin-bottom:0;
    }
    .nav-tabs-custom{
        box-shadow: 0 1px 1px rgba(0,0,0,0.0);
    }
    .progress{
        margin:5px 0;
    }
    .tab-pane-three{
        padding:30px 50px;
    }
    .tab-pane-three-div .span-title{
        font-weight: 900;
        font-size: 16px;
        height:30px;
    }
    .tab-pane-three-div li  .satisfy-col{
        height:30px;
        line-height: 30px;
        width:180px;
    }
    .checkbox{
        margin-top:0;
        margin-bottom:10px;
    }
    .checkbox-inline+.checkbox-inline, .radio-inline+.radio-inline{
        margin-left:0;
    }.addQues{
         border:1px solid #ccc;
         min-height:100px;
         margin:20px 10%;
     }
    .tijiao{
        margin-bottom:20px;
    }
    .zidingyi{
        border-top:1px solid #ddd;
    }
    .nav-title li a{
        font-size: 20px;
        font-weight: 900;
    }
    .prog{
        padding-left:0;
        padding-right:0;
    }
    .questionTitle{display:inline;_zoom:1;display:inline-block;}
</style>


<!-- Site wrapper -->
<div class="wrapper">

	#parse("/WEB-INF/views/common/leftbar.vm")

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                行程管理
            </h1>
            <ol class="breadcrumb">
                <li><a href="$!{link.contextPath}/schedule/mainPage"><i class="fa fa-dashboard"></i> 首页</a></li>
                <li class="active">行程管理</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">满意度调研</h3>
                    <!--<div class="box-tools"><a class="btn btn-default btn-sm" href="#">预览</a></div>-->
                </div>
                <!-- /.box-header -->

                <div class="box-body  content">
                    <div class="col-md-12">
                        <!-- Custom Tabs -->
                        <div class="nav-tabs-custom">
                            <ul class="nav nav-tabs nav-title">
                                <li class=""><a href="$!{link.contextPath}/survey/surveyQueryOrSave?teamId=$!teamId&title=$!title">问卷设计</a></li>
                                <li class="active"><a href="$!{link.contextPath}/survey/getQuestionnaireList?surveyId=$surveyId">问卷回收</a></li>
                               <!--- <li class=""><a href="#$surveyId">问卷统计</a></li>-->
                            </ul>
                            <div class="tab-content">
                                <!-- /.tab-pane -->
                                <div class="tab-pane  tab-pane-two" style="display:block;">
                                    #if($list.size()>0)
                                        #set ($i=0)

									#foreach( $d in $list)


                                        #if($!{d.thumbnailUrl})
                                            #set($i=$i+1)
                                            <a href="$!{d.pdfUrl}" class="mediapdf">
                                        <div class="col-md-3 col-xs-4 col-sm-3 col-lg-2">
                                            <img src="$!{d.thumbnailUrl}" alt="$!{d.userName}" />
                                            <p>$!{d.userName}.pdf</p>
                                        </div>
                                            </a>
                                            <!--<iframe style="display:none" id="printIframe$!{i}" src="$!{d.pdfUrl}"></iframe>-->
                                        #end

                                    #end
                                    <!--<div class="col-xs-12" style="margin-bottom: 20px;">
                                        <div class="col-xs-2 col-xs-offset-5">
                                            <button type="button" class="btn btn-block btn-info btn-lg">打印问卷</button>
                                        </div>
                                    </div>-->
                                        #else
                                        <div class="col-md-12 text-center">暂无问卷</div>
                                    #end
                                </div>
                                <!-- /.tab-pane -->
                            </div>
                            <!-- /.tab-content -->
                        </div>
                        <!-- nav-tabs-custom -->
                    </div>
                </div>

            </div>
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
	#parse("/WEB-INF/views/common/footer.vm")


    <!-- Add the sidebar's background. This div must be placed
   immediately after the control sidebar -->
    <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->



